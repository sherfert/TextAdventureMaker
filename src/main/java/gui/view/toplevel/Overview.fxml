<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import gui.customui.NamedObjectChooser?>


<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:id="mapBorderPane">
	<center>
		<ScrollPane id="outerMapPane">
			<Pane fx:id="mapPane" />
		</ScrollPane>
	</center>
	<bottom>
		<HBox spacing="10.0">
			<Button fx:id="newWayButton" mnemonicParsing="false" text="Create a way" />
			<Button fx:id="newLocationButton" mnemonicParsing="false"
				text="Create a location" />
			<padding>
				<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
			</padding>
		</HBox>

	</bottom>
</BorderPane>


